<!--form new address start-->
<div class="mx-auto" style="width: 80%">
    <form>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="inputEmail4">URL</label>
                <input type="text" class="form-control" id="inputURL" placeholder="URL">
            </div>
            <div class="form-group col-md-6">
                <label for="inputPassword4">Port</label>
                <input type="text" class="form-control" id="inputPORT" placeholder="port">
            </div>
        </div>

        <button type="submit" class="btn btn-primary">Добавить новый адрес</button>
    </form>
</div>
<!--form new address end-->

